      "source": [
        "for i in range(0, len(dataset1)):\n",
        "  for value in dataset1['x1'].unique():\n",
        "    dataset1.loc[dataset1['x1'] == value, 'x1_TE'] = (0.5 * dataset1['y'].mean()) + (1-0.5)*dataset1.loc[dataset1['x1'] == value]['y'].mean()\n",
        "    dataset1.loc[dataset1['x1'] == value, 'x1_x2_FE'] = (0.5 * dataset1['x2'].mean()) + (1-0.5)*dataset1.loc[dataset1['x1'] == value]['x2'].mean()\n",
        "dataset1"
